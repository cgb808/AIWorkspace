# Fast local dev env (ephemeral)
STRICT_ENV=false
# Postgres stub (started via `make pg-up`); falls back to sqlite if you comment this line
DATABASE_URL=postgresql://zenglow:dev@127.0.0.1:5432/zenglow
REDIS_HOST=127.0.0.1
REDIS_PORT=6379
DEV_FAKE_LLM=false
LLM_DEBUG=1
LLM_ALLOW_EXTERNAL=0
# Optional disable list: edge,ollama,llama (comma separated)
LLM_DISABLE=
