# Dashboard Environment Configuration
# Frontend (Vite) Environment Variables

# Supabase Configuration
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key-here

# API Base URLs
VITE_RAG_API_BASE=http://localhost:8000
VITE_GATEWAY_BASE=http://localhost:8000

# Health Monitoring Configuration
VITE_USE_AGG_HEALTH=false
VITE_DISABLE_WS_METRICS=false

# Optional: Metrics Configuration
VITE_METRICS_INTERVAL=4000
VITE_METRICS_FALLBACK_INTERVAL=10000

# Development overrides
VITE_DEV_MODE=true
VITE_LOG_LEVEL=debug
